@charset "utf-8";

:root { --primary-color: #2c3e50; --secondary-color: #3498db; --accent-color: #e74c3c; --background-color: #f9f9f9; --text-color: #333; --link-color: #2980b9; --link-hover-color: #3498db; --code-background: #f0f0f0; --border-color: #ddd; }

* { box-sizing: border-box; margin: 0px; padding: 0px; }

body { font-family: "Segoe UI", Tahoma, Geneva, Verdana, sans-serif; line-height: 1.6; color: var(--text-color); background-color: var(--background-color); max-width: 1200px; margin: 0px auto; padding: 20px; }

header { text-align: center; margin-bottom: 2rem; padding: 2rem 0px; border-bottom: 1px solid var(--border-color); }

header h1 { color: var(--primary-color); font-size: 2.5rem; margin-bottom: 0.5rem; }

.subtitle { font-size: 1.2rem; color: var(--secondary-color); }

nav { background-color: white; padding: 1.5rem; margin-bottom: 2rem; border-radius: 5px; box-shadow: rgba(0, 0, 0, 0.1) 0px 2px 5px; }

nav h2 { color: var(--primary-color); margin-bottom: 1rem; border-bottom: 1px solid var(--border-color); padding-bottom: 0.5rem; }

nav ul { list-style-type: none; }

nav ul li { margin-bottom: 0.5rem; }

nav ul li a { color: var(--link-color); text-decoration: none; display: block; padding: 0.5rem; border-radius: 3px; transition: background-color 0.2s; }

nav ul li a:hover { background-color: rgba(52, 152, 219, 0.1); color: var(--link-hover-color); }

.appendix { margin-top: 1rem; font-style: italic; }

main { background-color: white; padding: 2rem; border-radius: 5px; box-shadow: rgba(0, 0, 0, 0.1) 0px 2px 5px; }

section { margin-bottom: 2.5rem; }

h2 { color: var(--primary-color); margin-bottom: 1rem; padding-bottom: 0.5rem; border-bottom: 1px solid var(--border-color); }

h3 { color: var(--secondary-color); margin: 1.5rem 0px 1rem; }

p { margin-bottom: 1rem; }

ul, ol { margin-left: 2rem; margin-bottom: 1rem; }

a { color: var(--link-color); text-decoration: none; }

a:hover { color: var(--link-hover-color); text-decoration: underline; }

.diagram { text-align: center; margin: 2rem 0px; }

.diagram img { max-width: 100%; height: auto; border-radius: 5px; }

.caption { font-style: italic; color: rgb(102, 102, 102); text-align: center; margin-top: 0.5rem; }

code { font-family: "Courier New", Courier, monospace; background-color: var(--code-background); padding: 0.2rem 0.4rem; border-radius: 3px; }

pre { background-color: var(--code-background); padding: 1rem; border-radius: 5px; overflow-x: auto; margin-bottom: 1rem; }

blockquote { border-left: 4px solid var(--secondary-color); padding-left: 1rem; margin-left: 0px; margin-bottom: 1rem; color: rgb(85, 85, 85); }

.note { background-color: rgba(52, 152, 219, 0.1); border-left: 4px solid var(--secondary-color); padding: 1rem; margin: 1rem 0px; border-radius: 0px 5px 5px 0px; }

.warning { background-color: rgba(231, 76, 60, 0.1); border-left: 4px solid var(--accent-color); padding: 1rem; margin: 1rem 0px; border-radius: 0px 5px 5px 0px; }

table { width: 100%; border-collapse: collapse; margin-bottom: 1rem; }

th, td { padding: 0.75rem; text-align: left; border-bottom: 1px solid var(--border-color); }

th { background-color: var(--code-background); color: var(--primary-color); }

footer { text-align: center; margin-top: 2rem; padding: 1rem 0px; color: rgb(119, 119, 119); font-size: 0.9rem; border-top: 1px solid var(--border-color); }

@media (max-width: 768px) {
  body { padding: 10px; }
  header h1 { font-size: 2rem; }
  nav, main { padding: 1rem; }
}

@media print {
  body { background-color: white; color: black; }
  nav { display: none; }
  main { box-shadow: none; }
  a { color: black; text-decoration: none; }
  header, footer { text-align: center; }
}

.chapter-title { margin-top: 3rem; padding-top: 2rem; border-top: 2px solid var(--border-color); }

.chapter-title:first-of-type { margin-top: 0px; padding-top: 0px; border-top: none; }

.toc-link { font-weight: bold; }

#table-of-contents { position: relative; margin-bottom: 2rem; }

.back-to-top { position: fixed; bottom: 20px; right: 20px; background-color: var(--primary-color); color: white; width: 40px; height: 40px; border-radius: 50%; display: flex; justify-content: center; align-items: center; text-decoration: none; font-weight: bold; box-shadow: rgba(0, 0, 0, 0.2) 0px 2px 5px; z-index: 1000; }

.back-to-top:hover { background-color: var(--secondary-color); color: white; text-decoration: none; }
